{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "files": [
    "./node_modules/@kintone/dts-gen/kintone.d.ts",
  ],
  "compilerOptions": {
    "types": [
      "unplugin-icons/types/vue",
    ],
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}